    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "@uniswap/v3-sdk": "^3.25.2",
    "dotenv": "^16.0.3",
    "ethers": "^5.8.0",
    "hardhat-preprocessor": "^0.1.5",
    "jsbi": "^4.3.2",
    "merkletreejs": "^0.5.2",
    "uniswap": "^0.0.1"
